<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    [템플릿 설명]
    - 해당 파일은 SQL 문을 작성하는 곳입니다.
-->
<mapper namespace="com.example.rod.mybatis.mapper.UserMapper">

    <select id="selectUser" resultType="com.example.rod.mybatis.vo.UserMapperVO">
        SELECT userId,
               username,
               password
        FROM user
        WHERE id = #{id}
    </select>

